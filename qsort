#include<iostream>
using namespace std;

void swap(int arr[], int i,int j)
{
int t=arr[i];
arr[i]=arr[j];
arr[j]=t;
}

int partition(int arr[], int left,int right)
{
int i=left;
int j=right+1;
int x=arr[left];
int q=left;
while(1)
{
while(i<j&&arr[++i]>x);
while(arr[--j]<x);
if(i>=j)break;
swap(arr,i,j);
}
swap(arr,q,j);
return j;
}

void qsort(int arr[], int left,int right)
{if(left<right){
int q=partition(arr,left,right);
qsort(arr,left,q-1);
qsort(arr,q+1,right);
}
}

int main()
{
   int arr[]={5,4,8,7,9,2,6,1,3};
qsort(arr,0,8);
for(int i=0;i<9;i++)
cout<<arr[i]<<" ";
cout<<endl;
return 0;

}
